{
  "name": "build-wallet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "packages:build": "pnpm -r --filter './packages/*' run build",
    "packages:clean": "pnpm -r --filter './packages/*' run clean",
    "packages:dev": "pnpm -r --filter './packages/*' run dev",
    "version:bump": "bash scripts/version-bump.sh",
    "publish:check": "bash scripts/check-publish.sh",
    "publish:integrity": "node scripts/check-package-integrity.js",
    "publish:prepare": "node scripts/prepare-publish.js",
    "publish:restore": "node scripts/restore-workspace.js",
    "publish:all": "bash scripts/publish.sh"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^4.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-data-grid": "^8.5.1",
    "@mysten/sui": "^1.43.2",
    "@phoenix-wallet/aptos": "workspace:*",
    "@phoenix-wallet/core": "workspace:*",
    "@phoenix-wallet/evm": "workspace:*",
    "@phoenix-wallet/privy": "workspace:*",
    "@phoenix-wallet/solana": "workspace:*",
    "@phoenix-wallet/sui": "workspace:*",
    "@solana/spl-token": "^0.4.13",
    "@solana/wallet-adapter-phantom": "^0.9.27",
    "@solana/wallet-standard-wallet-adapter-base": "^1.1.4",
    "@solana/web3.js": "^1.98.2",
    "@wallet-standard/core": "^1.1.1",
    "ethers": "^6.14.3",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "viem": "^2.30.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@nx/devkit": "^22.0.2",
    "@nx/js": "^22.0.2",
    "@nx/workspace": "^22.0.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "nx": "^22.0.2",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
