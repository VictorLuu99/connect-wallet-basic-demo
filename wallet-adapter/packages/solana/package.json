{
  "name": "@phoenix-wallet/solana",
  "version": "1.0.5",
  "description": "Solana chain support for Phoenix Wallet",
  "main": "./dist/cjs/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json"
  },
  "dependencies": {
    "@phoenix-wallet/core": "workspace:*",
    "@solana/wallet-adapter-base": "^0.9.26",
    "@solana/web3.js": "^1.98.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "keywords": [
    "wallet",
    "solana",
    "web3",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tslib": "^2.8.1"
  },
  "module": "./dist/esm/src/index.js",
  "sideEffects": false
}
