{
  "name": "@phoenix-wallet/privy",
  "version": "1.0.5",
  "description": "Privy integration for Phoenix Wallet - Embedded wallet support",
  "main": "./dist/cjs/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json"
  },
  "dependencies": {
    "@phoenix-wallet/core": "workspace:*",
    "@privy-io/react-auth": "^2.17.2",
    "@solana-program/system": "^0.9.0",
    "@solana-program/token": "^0.7.0",
    "@solana/kit": "^5.0.0",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "@phoenix-wallet/evm": "workspace:*",
    "@phoenix-wallet/solana": "workspace:*",
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "@phoenix-wallet/evm": {
      "optional": true
    },
    "@phoenix-wallet/solana": {
      "optional": true
    }
  },
  "keywords": [
    "wallet",
    "privy",
    "embedded-wallet",
    "web3",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/esm/src/index.js",
  "sideEffects": false
}
